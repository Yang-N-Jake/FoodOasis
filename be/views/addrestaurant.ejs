							<script>
								function initMap() {
									var uluru = {lat: 23.5, lng:121};
									var map = new google.maps.Map(document.getElementById('map'), {
										zoom: 8,
										center: uluru
									});
									var marker = new google.maps.Marker({
										position: uluru,
										map: map
									});
								}
								google.maps.event.addDomListener(window, 'load', initMap);
							</script>
							
							
